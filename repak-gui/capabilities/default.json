  "identifier": "default",
  "description": "Capability for the main application window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "fs:allow-read-dir",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-exists",
    "fs:allow-remove",
    "fs:allow-create",
    "fs:allow-mkdir",
    "shell:default"
  ]
}
